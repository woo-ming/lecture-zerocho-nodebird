{
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": [
    "browser": true,
    "es2022": true,
    "node": true
  ],
  "extends": [
    "eslint: recommended",
    "plugin: react/recommended"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ]
}
